{
  "name": "@anpdgovbr/backlog-dim",
  "version": "0.5.47",
  "private": true,
  "type": "module",
  "description": "Aplicação para gerenciar processos relacionados à peticionamento e denúncias sobre a LGPD",
  "keywords": [
    "react",
    "crud",
    "typescript",
    "nextjs",
    "lgpd",
    "anpd",
    "dim",
    "admin",
    "prisma",
    "postgresql",
    "govbr",
    "dashboard",
    "auth",
    "material-ui"
  ],
  "homepage": "https://github.com/anpdgovbr/backlog-dim",
  "repository": {
    "type": "git",
    "url": "https://github.com/anpdgovbr/backlog-dim.git"
  },
  "author": "ANPD (Agência Nacional de Proteção de Dados) <desenvolvimento@anpd.gov.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anpdgovbr/backlog-dim/issues"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma generate && cross-env NEXT_DISABLE_TURBOPACK=1 next build && pnpm run postbuild",
    "postbuild": "node scripts/copy-public-to-standalone.cjs",
    "build-routes": "node scripts/generateDevRoutes.cjs",
    "build-routes:verbose": "node scripts/generateDevRoutes.cjs --verbose",
    "bump": "node scripts/bump-version.cjs",
    "bump:stage": "node scripts/bump-version-stage.cjs",
    "bump:patch": "node scripts/bump-version-advanced.cjs patch",
    "bump:minor": "node scripts/bump-version-advanced.cjs minor",
    "bump:major": "node scripts/bump-version-advanced.cjs major",
    "db:seed": "npx tsx prisma/seed.ts",
    "dev": "pnpm run prebuild && next dev --turbopack",
    "format": "prettier --write \"**/*.{ts,tsx,css,json}\"",
    "generate:version": "node scripts/generate-version.cjs",
    "lint": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint-staged": "lint-staged",
    "version:env": "node scripts/set-version-env.cjs",
    "prebuild": "pnpm run version:env && pnpm run generate:version && pnpm run build-routes",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:clean": "rimraf .typedocs public/docs || true",
    "docs:build": "npm run docs:typedoc",
    "docs": "npm run docs:build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "infra:setup": "node .infra/scripts/setup-cross-platform.cjs",
    "infra:setup:manual": "node .infra/scripts/setup-cross-platform.cjs --manual",
    "infra:setup:force": "node .infra/scripts/setup-cross-platform.cjs --force --auto",
    "infra:up": "node .infra/scripts/docker-helper.cjs up",
    "infra:down": "node .infra/scripts/docker-helper.cjs down",
    "infra:logs": "node .infra/scripts/docker-helper.cjs logs",
    "infra:reset": "node .infra/scripts/docker-helper.cjs reset",
    "infra:clean": "node .infra/scripts/docker-helper.cjs clean",
    "infra:psql": "node .infra/scripts/docker-helper.cjs psql",
    "infra:status": "node .infra/scripts/docker-helper.cjs status",
    "infra:backup": "node .infra/scripts/docker-helper.cjs backup",
    "infra:db:init": "node .infra/scripts/db-helper.cjs setup",
    "infra:db:fresh": "node .infra/scripts/db-helper.cjs fresh",
    "infra:db:migrate": "node .infra/scripts/db-helper.cjs migrate",
    "infra:db:seed": "node .infra/scripts/db-helper.cjs seed",
    "infra:db:studio": "node .infra/scripts/db-helper.cjs studio",
    "infra:db:reset": "node .infra/scripts/db-helper.cjs reset",
    "infra:update": "node .infra/scripts/update-scripts.cjs",
    "infra:debug": "node .infra/scripts/diagnostic.cjs",
    "infra:fix": "node .infra/scripts/fix-credentials.cjs"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anpdgovbr/rbac-admin": "0.4.3",
    "@anpdgovbr/rbac-core": "^0.2.0",
    "@anpdgovbr/rbac-next": "^0.2.0",
    "@anpdgovbr/rbac-prisma": "^0.2.0",
    "@anpdgovbr/rbac-provider": "^0.2.0",
    "@anpdgovbr/rbac-react": "^0.3.0",
    "@anpdgovbr/shared-types": "0.3.1-beta.1",
    "@anpdgovbr/shared-ui": "0.3.13-beta.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@govbr-ds/core": "^3.6.2",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "@mui/x-data-grid": "^8.17.0",
    "@prisma/client": "6.19.0",
    "critters": "^0.0.25",
    "dayjs": "^1.11.19",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "^16.0.1",
    "next-auth": "^4.24.13",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-lgpd-consent": "^0.5.4",
    "recharts": "^3.4.1",
    "remask": "^1.2.2",
    "swr": "^2.3.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/codemod": "^16.0.1",
    "@next/eslint-plugin-next": "^16.0.1",
    "@types/chai": "^5.2.3",
    "@types/d3-array": "^3.2.2",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-path": "^3.1.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/d3-timer": "^3.0.2",
    "@types/estree": "^1.0.8",
    "@types/hast": "^3.0.4",
    "@types/intl": "^1.2.2",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^2.2.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/papaparse": "^5.5.0",
    "@types/parse-json": "^7.0.0",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-transition-group": "^4.4.12",
    "@types/unist": "^3.0.3",
    "@types/use-sync-external-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prisma": "6.19.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
