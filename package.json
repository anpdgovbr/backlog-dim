{
  "name": "backlog-dim",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run buil-routes && next dev --turbopack",
    "devs": "node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,css,json}\"",
    "pre-commit": "lint-staged",
    "lint-staged": "lint-staged",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase stop && npx supabase start",
    "supabase:clean": "docker ps --filter \"name=supabase\" -aq | ForEach-Object { docker stop $_; docker rm $_ } && docker network prune -f",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "npx tsx prisma/seed.ts",
    "build-routes": "tsx scripts/generateDevRoutes.ts",
    "prebuild": "npm run build-routes"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "next lint --fix",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@azure/msal-node": "^3.2.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@govbr-ds/core": "^3.6.1",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@mui/x-data-grid": "^7.28.0",
    "@prisma/client": "^6.5.0",
    "@supabase/supabase-js": "^2.49.1",
    "multer": "^1.4.5-lts.1",
    "next": "^15.2.1",
    "next-auth": "^4.24.7",
    "papaparse": "^5.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-input-mask": "^2.0.4",
    "sass": "^1.85.1",
    "text-mask-addons": "^3.8.0",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.9",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-input-mask": "^3.0.6",
    "@types/react-text-mask": "^5.4.14",
    "@types/text-mask-addons": "^3.8.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "purgecss": "^7.0.2",
    "supabase": "^2.15.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
