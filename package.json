{
  "name": "@anpdgovbr/backlog-dim",
  "version": "0.4.16",
  "private": true,
  "type": "module",
  "description": "Aplicação para gerenciar processos relacionados à peticionamento e denúncias sobre a LGPD",
  "keywords": [
    "react",
    "crud",
    "typescript",
    "nextjs",
    "lgpd",
    "anpd",
    "dim",
    "admin",
    "prisma",
    "postgresql",
    "govbr",
    "dashboard",
    "auth",
    "material-ui"
  ],
  "homepage": "https://github.com/anpdgovbr/backlog-dim",
  "repository": {
    "type": "git",
    "url": "https://github.com/anpdgovbr/backlog-dim.git"
  },
  "author": "ANPD (Autoridade Nacional de Proteção de Dados) <desenvolvimento@anpd.gov.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anpdgovbr/backlog-dim/issues"
  },
  "scripts": {
    "build": "prisma generate && next build",
    "build-routes": "node scripts/generateDevRoutes.cjs",
    "build-routes:verbose": "node scripts/generateDevRoutes.cjs --verbose",
    "bump": "node scripts/bump-version.cjs",
    "bump:stage": "node scripts/bump-version-stage.cjs",
    "bump:patch": "node scripts/bump-version-advanced.cjs patch",
    "bump:minor": "node scripts/bump-version-advanced.cjs minor",
    "bump:major": "node scripts/bump-version-advanced.cjs major",
    "db:seed": "npx tsx prisma/seed.ts",
    "dev": "npm run prebuild && next dev --turbopack",
    "format": "prettier --write \"**/*.{ts,tsx,css,json}\"",
    "generate:version": "node scripts/generate-version.cjs",
    "lint": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint-staged": "lint-staged",
    "version:env": "node scripts/set-version-env.cjs",
    "prebuild": "npm run version:env && npm run generate:version && npm run build-routes",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:clean": "rimraf .typedocs public/docs || true",
    "docs:build": "npm run docs:typedoc",
    "docs": "npm run docs:build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "infra:setup": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/setup-cross-platform.js -o .infra/setup-cross-platform.cjs && curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/docker-helper.js -o .infra/docker-helper.cjs && curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/db-helper.js -o .infra/db-helper.cjs && node .infra/setup-cross-platform.cjs",
    "infra:setup:manual": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/setup-cross-platform.js -o .infra/setup-cross-platform.cjs && curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/docker-helper.js -o .infra/docker-helper.cjs && curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/db-helper.js -o .infra/db-helper.cjs && node .infra/setup-cross-platform.cjs --manual",
    "infra:setup:force": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/setup-cross-platform.js -o .infra/setup-cross-platform.cjs && curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/docker-helper.js -o .infra/docker-helper.cjs && curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/db-helper.js -o .infra/db-helper.cjs && node .infra/setup-cross-platform.cjs --force --auto",
    "infra:up": "node .infra/docker-helper.cjs up",
    "infra:down": "node .infra/docker-helper.cjs down",
    "infra:logs": "node .infra/docker-helper.cjs logs",
    "infra:reset": "node .infra/docker-helper.cjs reset",
    "infra:clean": "node .infra/docker-helper.cjs clean",
    "infra:psql": "node .infra/docker-helper.cjs psql",
    "infra:status": "node .infra/docker-helper.cjs status",
    "infra:backup": "node .infra/docker-helper.cjs backup",
    "infra:db:init": "node .infra/db-helper.cjs setup",
    "infra:db:fresh": "node .infra/db-helper.cjs fresh",
    "infra:db:migrate": "node .infra/db-helper.cjs migrate",
    "infra:db:seed": "node .infra/db-helper.cjs seed",
    "infra:db:studio": "node .infra/db-helper.cjs studio",
    "infra:db:reset": "node .infra/db-helper.cjs reset",
    "infra:update": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/update-scripts.js -o .infra/update-scripts.cjs && node .infra/update-scripts.cjs",
    "infra:debug": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/diagnostic.js -o .infra/diagnostic.cjs && node .infra/diagnostic.cjs",
    "infra:fix": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/fix-credentials.js -o .infra/fix-credentials.cjs && node .infra/fix-credentials.cjs"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anpdgovbr/rbac-admin": "^0.2.1-beta.0",
    "@anpdgovbr/rbac-core": "^0.1.2",
    "@anpdgovbr/rbac-next": "^0.1.2",
    "@anpdgovbr/rbac-prisma": "^0.1.2",
    "@anpdgovbr/rbac-provider": "^0.1.2",
    "@anpdgovbr/rbac-react": "^0.2.2",
    "@anpdgovbr/shared-types": "^0.1.7-beta-4",
    "@anpdgovbr/shared-ui": "^0.3.4-beta.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@govbr-ds/core": "^3.6.2",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.2",
    "@prisma/client": "6.16.1",
    "dayjs": "^1.11.18",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "^15.5.3",
    "next-auth": "^4.24.11",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-lgpd-consent": "^0.3.7",
    "recharts": "^3.2.0",
    "remask": "^1.2.2",
    "swr": "^2.3.6",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@next/eslint-plugin-next": "^15.5.3",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.1",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prisma": "6.16.1",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
