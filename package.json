{
  "name": "backlog-dim",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,css,json}\"",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase stop && npx supabase start",
    "supabase:clean": "docker ps --filter \"name=supabase\" -aq | ForEach-Object { docker stop $_; docker rm $_ } && docker network prune -f",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "npx tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/core": "^0.38.0",
    "@auth/nextjs": "^0.0.0-380f8d56",
    "@azure/msal-node": "^3.2.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@govbr-ds/core": "^3.6.1",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@mui/x-data-grid": "^7.27.2",
    "@prisma/client": "^6.4.1",
    "@supabase/supabase-js": "^2.49.1",
    "multer": "^1.4.5-lts.1",
    "next": "15.2.1",
    "next-auth": "^4.24.11",
    "papaparse": "^5.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.9",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.2.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prisma": "^6.4.1",
    "purgecss": "^7.0.2",
    "supabase": "^2.15.8",
    "tailwindcss": "^4.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
