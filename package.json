{
  "name": "@anpdgovbr/backlog-dim",
  "version": "0.2.89",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=1 npx prisma generate && next build",
    "build-routes": "tsx scripts/generateDevRoutes.ts",
    "bump": "node scripts/bump-version.cjs",
    "bump:patch": "node scripts/bump-version-advanced.cjs patch",
    "bump:minor": "node scripts/bump-version-advanced.cjs minor",
    "bump:major": "node scripts/bump-version-advanced.cjs major",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "npx tsx prisma/seed.ts",
    "dev": "npm run build-routes && next dev --turbopack",
    "dev:https": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node scripts/dev-server.mjs",
    "devs": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node server.js",
    "fix:mui-imports": "node scripts/fix-mui-imports.mjs",
    "fix:mui-imports:dry": "node scripts/fix-mui-imports.mjs --dry-run",
    "fix:mui-imports:advanced": "node scripts/fix-mui-imports-advanced.mjs",
    "fix:mui-imports:advanced:dry": "node scripts/fix-mui-imports-advanced.mjs --dry-run",
    "format": "prettier --write \"**/*.{ts,tsx,css,json}\"",
    "generate:version": "node scripts/generate-version.cjs",
    "lint": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint-staged": "lint-staged",
    "version:env": "node scripts/set-version-env.cjs",
    "prebuild": "npm run version:env && npm run generate:version && npm run build-routes",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "infra:setup": "curl -sSL https://raw.githubusercontent.com/anpdgovbr/docker-infra-pg/main/setup-infra.sh | bash",
    "infra:up": "cd infra-db && docker-compose up -d",
    "infra:down": "cd infra-db && docker-compose down",
    "infra:logs": "cd infra-db && docker-compose logs -f postgres",
    "infra:reset": "cd infra-db && docker-compose down -v && docker-compose up -d",
    "db:setup": "npm run infra:up && sleep 5 && npm run prisma:migrate && npm run prisma:seed",
    "db:fresh": "npm run infra:reset && sleep 10 && npm run db:setup"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anpdgovbr/shared-types": "^0.1.4-beta",
    "@azure/msal-node": "^3.6.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@govbr-ds/core": "^3.6.2",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.9.1",
    "@prisma/client": "^6.13.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "next": "^15.4.4",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.61.1",
    "react-input-mask": "^2.0.4",
    "recharts": "^3.1.0",
    "remask": "^1.2.2",
    "sass": "^1.89.2",
    "swr": "^2.3.4",
    "text-mask-addons": "^3.8.0",
    "tsx": "^4.20.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.4.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^24.1.0",
    "@types/node-fetch": "^2.6.12",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-input-mask": "^3.0.6",
    "@types/react-text-mask": "^5.4.14",
    "@types/text-mask-addons": "^3.8.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "purgecss": "^7.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
